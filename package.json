{
  "name": "red-contrib-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Node-RED contribution packages",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "publish:all": "npm publish --workspaces",
    "publish:dry": "npm publish --workspaces --dry-run",
    "publish:questdb": "npm publish -w node-red-contrib-questdb",
    "publish:compression": "npm publish -w node-red-contrib-compression",
    "publish:connect": "npm publish -w node-red-contrib-connect"
  },
  "keywords": [
    "node-red"
  ],
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "asynckit": "^0.4.0",
    "axios": "^1.13.2",
    "call-bind-apply-helpers": "^1.0.2",
    "combined-stream": "^1.0.8",
    "delayed-stream": "^1.0.0",
    "dunder-proto": "^1.0.1",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es-object-atoms": "^1.1.1",
    "es-set-tostringtag": "^2.1.0",
    "follow-redirects": "^1.15.11",
    "form-data": "^4.0.5",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.3.0",
    "get-proto": "^1.0.1",
    "gopd": "^1.2.0",
    "has-symbols": "^1.1.0",
    "has-tostringtag": "^1.0.2",
    "hasown": "^2.0.2",
    "math-intrinsics": "^1.1.0",
    "mime-db": "^1.52.0",
    "mime-types": "^2.1.35",
    "node-red-contrib-compression": "^0.1.0",
    "node-red-contrib-connect": "^0.1.1",
    "node-red-contrib-questdb": "^0.5.1",
    "proxy-from-env": "^1.1.0",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErnstHolger/node-red.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/ErnstHolger/node-red/issues"
  },
  "homepage": "https://github.com/ErnstHolger/node-red#readme"
}
